{
  "name": "shawnbot",
  "version": "1.0.0",
  "description": "Discord AFK kick bot",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "start": "tsx src/index.ts",
    "dev": "tsx src/index.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "dev:all": "concurrently -n bot,webui -c blue,magenta \"npm run dev\" \"npm run dev --prefix webui\""
  },
  "keywords": [
    "discord",
    "bot",
    "afk"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@discordjs/voice": "^0.18.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^11.0.0",
    "discord.js": "^14.14.0",
    "dotenv": "^16.4.0",
    "express": "^5.2.1",
    "pino": "^9.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/better-sqlite3": "^7.6.11",
    "@types/express": "^5.0.6",
    "@types/node": "^22.0.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.57.0",
    "pino-pretty": "^11.0.0",
    "supertest": "^7.1.4",
    "tsx": "^4.19.0",
    "typescript": "^5.5.0",
    "vitest": "^4.0.16"
  }
}
